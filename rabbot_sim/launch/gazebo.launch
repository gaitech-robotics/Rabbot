<?xml version="1.0"?>
<launch>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="use_sim_time" value="true"/>
    <arg name="debug" value="false"/>
    <arg name="paused" value="false"/>
    <arg name="gui" value="true"/>
  </include>

  <param name="robot_description" command="$(find xacro)xacro '$(find rabbot_description)/include/urdf/rabbot_gazebo.urdf.xacro'"/>

  <node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen" args="-urdf -model rabbot -param robot_description"/>

  <rosparam file="$(find rabbot_sim)/controller/rabbot_controller.yaml" command="load"/>

  <node name="base_controller_spawner" pkg="controller_manager" type="spawner"  args="joint_state_controller rabbot_base_controller rabbot_arm_controller rabbot_gripper_controller" respawn="false" output="screen"/>

  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"/>
 

</launch>
